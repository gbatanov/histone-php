<?xml version="1.0" encoding="UTF-8"?>
<suites>
	<suite name="selectors">
		<case>
			<input>a{{asdfsadfasdfdsfdasf}}b</input>
			<expected>ab</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb.foo}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb[2333]}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb['sdajfbdsuf']}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb[sadfsdf]}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb[sadfsdf.dsafds]}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb[sadfsdf['dsafds']]}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb[sadfsdf[98]]}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb.foo[111].addaf}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb[111].addaf}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{testasdfdsbafb.a.b.c[a.b.c[x[1111].yy.z[92]]].addaf[222]['sss'][adfaf].bb[1]}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{test}} b</input>
			<context>{"test": "foobar"}</context>
			<expected>a foobar b</expected>
		</case>

		<case>
			<input>a {{test.foo}} b</input>
			<context>{"test": {"foo" : 98}}</context>
			<expected>a 98 b</expected>
		</case>

		<case>
			<input>a {{test[4]}} b</input>
			<context>{"test": [1, 2, 3, 4, "foo", 5]}</context>
			<expected>a foo b</expected>
		</case>

		<case>
			<input>a {{test["foo"]}} b</input>
			<context>{"test": {"foo" : 98}}</context>
			<expected>a 98 b</expected>
		</case>

		<case>
			<input>a {{test[foo]}} b</input>
			<context>{"test": {"bar" : 98}, "foo": "bar"}</context>
			<expected>a 98 b</expected>
		</case>

		<case>
			<input>a {{test[foo.bar]}} b</input>
			<context>{"test": {"xxx" : 3.14}, "foo": {"bar": "xxx"}}</context>
			<expected>a 3.14 b</expected>
		</case>

		<case>
			<input>a {{test[foo['bar']]}} b</input>
			<context>{"test": {"xxx" : "---"}, "foo": {"bar": "xxx"}}</context>
			<expected>a --- b</expected>
		</case>

		<case>
			<input>a {{test[a[8]]}} b</input>
			<context>{"test": {"xxx" : "---"}, "a": [0, 1, 2, 3, 4, 5, 6, 7, "xxx", 9]}</context>
			<expected>a --- b</expected>
		</case>

		<case>
			<input>a {{test.foo[1].addaf}} b</input>
			<context>{"test": {"foo": ["+++", {"addaf": "---"}]}}</context>
			<expected>a --- b</expected>
		</case>

		<case>
			<input>a {{test['foo'][1].addaf}} b</input>
			<context>{"test": {"foo": ["+++", {"addaf": "---"}]}}</context>
			<expected>a --- b</expected>
		</case>

		<case>
			<input>a {{foo}} b</input>
			<context>{"foo": "bar"}</context>
			<expected>a bar b</expected>
		</case>

		<case>
			<input>a {{test.foo[1].x[1]['y']}} b</input>
			<context>{
				"test": {
					"foo": [
						"+++", {
							"addaf": "---",
							"x": [1, {
								"y": 0.14E3
							}, 3]
						}
					]
				}
			}</context>
			<expected>a 140 b</expected>
		</case>
	</suite>
</suites>