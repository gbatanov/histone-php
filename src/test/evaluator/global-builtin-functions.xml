<?xml version="1.0" encoding="UTF-8"?>
<suites>

	<suite name="min">
		<case>
			<input>a {{min()}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{min().isUndefined()}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{min(399).isNumber()}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{min(-10, 0, 10)}} b</input>
			<expected>a -10 b</expected>
		</case>

		<case>
			<input>a {{min('string', 0, 10)}} b</input>
			<expected>a 0 b</expected>
		</case>

		<case>
			<input>a {{min('string', 'foo', 10) is 10}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{min(1000, 200, 3000, 4000, 3, 1000, 200)}} b</input>
			<expected>a 3 b</expected>
		</case>

		<case>
			<input>a {{min(-33.30, 0, 1.30)}} b</input>
			<expected>a -33.3 b</expected>
		</case>

		<case>
			<input>a {{min('string', 0, 32.103)}} b</input>
			<expected>a 0 b</expected>
		</case>

		<case>
			<input>a {{min('string', 'foo', 13.03) is 13.03}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{min(100.03, 200.3, 3000.8, 4000.12, 3.2, 1000.2, 200.122)}} b</input>
			<expected>a 3.2 b</expected>
		</case>

		<case>
			<input>a {{min(-10E3, 0, 10E2)}} b</input>
			<expected>a -10000 b</expected>
		</case>

		<case>
			<input>a {{min('string', 0E3, 10E3)}} b</input>
			<expected>a 0 b</expected>
		</case>

		<case>
			<input>a {{min('string', 'foo', 10E3) is 10E3}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{min(1000E2, 200E2, 3000E+2, 4000E3, 3E+4, 1000E2, 200E0)}} b</input>
			<expected>a 200 b</expected>
		</case>

		<case>
			<input>a {{min(-33.30E1, 0E1, 1.30E3)}} b</input>
			<expected>a -333 b</expected>
		</case>

		<case>
			<input>a {{min('string', 0E+0, 32.103E3)}} b</input>
			<expected>a 0 b</expected>
		</case>

		<case>
			<input>a {{min('string', 'foo', 13.03E2) is 13.03E2}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{min(100.03E+8, 200.3E1, 3000.8, 4000.12E1, 3.2E0, 1000.2E+0, 200.122E2)}} b</input>
			<expected>a 3.2 b</expected>
		</case>

		<case>
			<input>a {{min(-10E-3, 0, 10E-2)}} b</input>
			<expected>a -0.01 b</expected>
		</case>

		<case>
			<input>a {{min('string', 0E-3, 10E-3)}} b</input>
			<expected>a 0 b</expected>
		</case>

		<case>
			<input>a {{min('string', 'foo', 10E-3) is 10E-3}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{min(1000E-2, 200E-2, 3000E-2, 4000E-3, 3E-4, 1000E-2, 200E-0)}} b</input>
			<expected>a 0.0003 b</expected>
		</case>

		<case>
			<input>a {{min(-33.30E-1, 0E-1, 1.30E-3)}} b</input>
			<expected>a -3.33 b</expected>
		</case>

		<case>
			<input>a {{min('string', 0E-0, 32.103E-3)}} b</input>
			<expected>a 0 b</expected>
		</case>

		<case>
			<input>a {{min('string', 'foo', 13.03E-2) is 13.03E-2}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{min(100.03E-8, 200.3E-1, 3000.8, 4000.12E-1, 3.2E-0, 1000.2E-0, 200.122E2)}} b</input>
			<expected>a 0.0000010003 b</expected>
		</case>
	</suite>

	<suite name="max">
		<case>
			<input>a {{max()}} b</input>
			<expected>a  b</expected>
		</case>

		<case>
			<input>a {{max().isUndefined()}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{max(-128).isNumber()}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{max(-10, 0, 10)}} b</input>
			<expected>a 10 b</expected>
		</case>

		<case>
			<input>a {{max('string', 0, 10)}} b</input>
			<expected>a 10 b</expected>
		</case>

		<case>
			<input>a {{max('string', 'foo', 10) is 10}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{max(1000, 200, 3000, 4000, 3, 1000, 200)}} b</input>
			<expected>a 4000 b</expected>
		</case>

		<case>
			<input>a {{max(-33.30, 0, 1.30)}} b</input>
			<expected>a 1.3 b</expected>
		</case>

		<case>
			<input>a {{max('string', 0, 32.103)}} b</input>
			<expected>a 32.103 b</expected>
		</case>

		<case>
			<input>a {{max('string', 'foo', 13.03) is 13.03}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{max(100.03, 200.3, 3000.8, 4000.12, 3.2, 1000.2, 200.122)}} b</input>
			<expected>a 4000.12 b</expected>
		</case>

		<case>
			<input>a {{max(-10E3, 0, 10E2)}} b</input>
			<expected>a 1000 b</expected>
		</case>

		<case>
			<input>a {{max('string', 0E3, 10E3)}} b</input>
			<expected>a 10000 b</expected>
		</case>

		<case>
			<input>a {{max('string', 'foo', 10E3) is 10E3}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{max(1000E2, 200E2, 3000E+2, 4000E3, 3E+4, 1000E2, 200E0)}} b</input>
			<expected>a 4000000 b</expected>
		</case>

		<case>
			<input>a {{max(-33.30E1, 0E1, 1.30E3)}} b</input>
			<expected>a 1300 b</expected>
		</case>

		<case>
			<input>a {{max('string', 0E+0, 32.103E3)}} b</input>
			<expected>a 32103 b</expected>
		</case>

		<case>
			<input>a {{max('string', 'foo', 13.03E2) is 13.03E2}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{max(100.03E+8, 200.3E1, 3000.8, 4000.12E1, 3.2E0, 1000.2E+0, 200.122E2)}} b</input>
			<expected>a 10003000000 b</expected>
		</case>

		<case>
			<input>a {{max(-10E-3, 0, 10E-2)}} b</input>
			<expected>a 0.1 b</expected>
		</case>

		<case>
			<input>a {{max('string', 0E-3, 10E-3)}} b</input>
			<expected>a 0.01 b</expected>
		</case>

		<case>
			<input>a {{max('string', 'foo', 10E-3) is 10E-3}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{max(1000E-2, 200E-2, 3000E-2, 4000E-3, 3E-4, 1000E-2, 200E-0)}} b</input>
			<expected>a 200 b</expected>
		</case>

		<case>
			<input>a {{max(-33.30E-1, 0E-1, 1.30E-3)}} b</input>
			<expected>a 0.0013 b</expected>
		</case>

		<case>
			<input>a {{max('string', 0E-0, 32.103E-3)}} b</input>
			<expected>a 0.032103 b</expected>
		</case>

		<case>
			<input>a {{max('string', 'foo', 13.03E-2) is 13.03E-2}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{max(100.03E-8, 200.3E-1, 3000.8, 4000.12E-1, 3.2E-0, 1000.2E-0, 200.122E2)}} b</input>
			<expected>a 20012.2 b</expected>
		</case>
	</suite>

	<suite name="dayOfWeek">
		<case>
			<input>a {{dayOfWeek(2012, 4, 2)}} b</input>
			<expected>a 1 b</expected>
		</case>

        <case>
			<input>a {{dayOfWeek(2012, 4, 3)}} b</input>
			<expected>a 2 b</expected>
		</case>

        <case>
			<input>a {{dayOfWeek(2012, 4, 4)}} b</input>
			<expected>a 3 b</expected>
		</case>

        <case>
			<input>a {{dayOfWeek(2012, 4, 5)}} b</input>
			<expected>a 4 b</expected>
		</case>

        <case>
			<input>a {{dayOfWeek(2012, 4, 6)}} b</input>
			<expected>a 5 b</expected>
		</case>

        <case>
			<input>a {{dayOfWeek(2012, 4, 7)}} b</input>
			<expected>a 6 b</expected>
		</case>

		<case>
			<input>a {{dayOfWeek(2012, 4, 8)}} b</input>
			<expected>a 7 b</expected>
		</case>

        <case>
            <input>a {{dayOfWeek(2012,1,1).isNumber()}} b</input>
            <expected>a true b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek(2012,1,1)}} b</input>
            <expected>a 7 b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek(2012,1,32).isUndefined()}} b</input>
            <expected>a true b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek(2012,2,29)}} b</input>
            <expected>a 3 b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek(2011,2,28)}} b</input>
            <expected>a 1 b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek(2011,2,29).isUndefined()}} b</input>
            <expected>a true b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek(2011,3,1)}} b</input>
            <expected>a 2 b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek('2011','3','1')}} b</input>
            <expected>a 2 b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek('a',3,1).isUndefined()}} b</input>
            <expected>a true b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek(2011,'a',1).isUndefined()}} b</input>
            <expected>a true b</expected>
        </case>

        <case>
            <input>a {{dayOfWeek(2011,3,'a').isUndefined()}} b</input>
            <expected>a true b</expected>
        </case>
	</suite>

	<suite name="daysInMonth">
		<case>
			<input>a {{daysInMonth(2012, 4)}} b</input>
			<expected>a 30 b</expected>
		</case>

		<case>
			<input>a {{daysInMonth(2012, 3)}} b</input>
			<expected>a 31 b</expected>
		</case>

		<case>
			<input>a {{daysInMonth(2012, 2)}} b</input>
			<expected>a 29 b</expected>
		</case>

		<case>
			<input>a {{daysInMonth(1900, 2)}} b</input>
			<expected>a 28 b</expected>
		</case>

		<case>
			<input>a {{daysInMonth(2011, 2)}} b</input>
			<expected>a 28 b</expected>
		</case>

        <case>
            <input>a {{daysInMonth(1900, 2).isNumber()}} b</input>
            <expected>a true b</expected>
        </case>

        <case>
            <input>a {{daysInMonth(1900,0).isUndefined()}} b</input>
            <expected>a true b</expected>
        </case>

        <case>
            <input>a {{daysInMonth(1900,13).isUndefined()}} b</input>
            <expected>a true b</expected>
        </case>

        <case>
            <input>a {{daysInMonth(1900).isUndefined()}} b</input>
            <expected>a true b</expected>
        </case>

        <case>
            <input>a {{daysInMonth('2012',2)}} b</input>
            <expected>a 29 b</expected>
        </case>

        <case>
            <input>a {{daysInMonth(2012,'2')}} b</input>
            <expected>a 29 b</expected>
        </case>

        <case>
            <input>a {{daysInMonth('2012','2')}} b</input>
            <expected>a 29 b</expected>
        </case>
	</suite>

	<suite name="uniqueId">
		<case>
			<input>a {{uniqueId().isString()}} b</input>
			<expected>a true b</expected>
		</case>

		<case>
			<input>a {{uniqueId().size()}} b</input>
			<expected>a 36 b</expected>
		</case>

		<case>
			<input>a {{uniqueId().split('-').size()}} b</input>
			<expected>a 5 b</expected>
		</case>

		<case>
			<input>a {{for item in uniqueId().split('-')}}{{item.size()}}-{{/for}} b</input>
			<expected>a 8-4-4-4-12- b</expected>
		</case>

		<case>
			<input>a {{uniqueId().strip('-0123456789abcdef')}} b</input>
			<expected>a  b</expected>
		</case>
	</suite>

    <suite name="range">
        <case>
            <input>a {{var x = range(1,1)}} b {{x}} c {{x is [1]}} d</input>
            <expected>a  b 1 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range(1,3)}} b {{x}} c {{x is [1,2,3]}} d</input>
            <expected>a  b 1 2 3 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range(3,1)}} b {{x}} c {{x is [3,2,1]}} d</input>
            <expected>a  b 3 2 1 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range(-10,-10)}} b {{x}} c {{x is [-10]}} d</input>
            <expected>a  b -10 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range(-3,-1)}} b {{x}} c {{x is [-3,-2,-1]}} d</input>
            <expected>a  b -3 -2 -1 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range(-1,-3)}} b {{x}} c {{x is [-1,-2,-3]}} d</input>
            <expected>a  b -1 -2 -3 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range(-2,2)}} b {{x}} c {{x is [-2,-1,0,1,2]}} d</input>
            <expected>a  b -2 -1 0 1 2 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range(2,-2)}} b {{x}} c {{x is [2,1,0,-1,-2]}} d</input>
            <expected>a  b 2 1 0 -1 -2 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range('1',2)}} b {{x}} c {{x is [1,2]}} d</input>
            <expected>a  b 1 2 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range(1,'2')}} b {{x}} c {{x is [1,2]}} d</input>
            <expected>a  b 1 2 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range('1','2')}} b {{x}} c {{x is [1,2]}} d</input>
            <expected>a  b 1 2 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range(1.1)}} b {{x.isUndefined()}} c</input>
            <expected>a  b true c</expected>
        </case>

        <case>
            <input>a {{var x = range(1.1,2)}} b {{x.isUndefined()}} c</input>
            <expected>a  b true c</expected>
        </case>

        <case>
            <input>a {{var x = range(1.1,2.2)}} b {{x.isUndefined()}} c</input>
            <expected>a  b true c</expected>
        </case>

        <case>
            <input>a {{var x = range(1,2.2)}} b {{x.isUndefined()}} c</input>
            <expected>a  b true c</expected>
        </case>

        <case>
            <input>a {{var x = range(1,3,'adsfasdfdsf')}} b {{x}} c {{x is [1,2,3]}} d</input>
            <expected>a  b 1 2 3 c true d</expected>
        </case>

        <case>
            <input>a {{var x = range('aaa',2)}} b {{x.isUndefined()}} c</input>
            <expected>a  b true c</expected>
        </case>

        <case>
            <input>a {{var x = range(2,'aaa')}} b {{x.isUndefined()}} c</input>
            <expected>a  b true c</expected>
        </case>

        <case>
            <input>a {{var x = range('aaa','bbb')}} b {{x.isUndefined()}} c</input>
            <expected>a  b true c</expected>
        </case>
    </suite>


</suites>